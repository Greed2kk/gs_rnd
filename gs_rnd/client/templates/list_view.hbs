<div class="search-area position-fixed">
  <div class="input-group search">
    <input type="text" class="search-bar" placeholder="Поиск">
  </div>
  <div class="sights-list"></div>
  <div class="container position-fixed"></div>

  <div id="paginator">

    <nav aria-label="Page navigation example">
      <ul class="pagination">
        <li class="page-item">
          <a class="page-link previous" href="#" aria-label="Previous">
            <span aria-hidden="true">&laquo;</span>
            <span class="sr-only">Previous</span>
          </a>
        </li>
        <li class="page-item"><span class="page-link pages">{{offset}}-{{top}}</span></li>
        <li class="page-item">
          <a class="page-link next" href="#" aria-label="Next">
            <span aria-hidden="true">
              &raquo;
            </span>
            <span class="sr-only">
              Next
            </span>
          </a>
        </li>
        <li class="page-item">
          <span class="page-link total">
            {{total}}
          </span>
        </li>
        {{#each limiter}}
        <li class="page-item limiters">
          {{#if this.selected}}
             <span class="page-link limiter active" data-limiter="{{this.value}}" data-id="{{@index}}">
               {{this.value}}
          </span>
          {{else}}
             <span class="page-link limiter" data-limiter="{{this.value}}" data-id="{{@index}}">
               {{this.value}}
          </span>
          {{/if}}
        </li>
        {{/each}}
      </ul>
    </nav>
  </div>